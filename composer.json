        "zaxbux/backblaze-b2-php": "^2.0.2"
    },
    "require-dev": {
        "league/flysystem-adapter-test-utilities": "^3.0",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
